#include <bits/stdc++.h>
using namespace std;

vector<vector<int>> mergeIntervals(vector<vector<int>>& intervals) {
    if (intervals.empty()) return {};

    // Step 1: Sort intervals by start
    sort(intervals.begin(), intervals.end());

    vector<vector<int>> result;
    result.push_back(intervals[0]); // first interval

    // Step 2: Traverse and merge
    for (int i = 1; i < intervals.size(); i++) {
        // If current interval overlaps with the last one in result
        if (intervals[i][0] <= result.back()[1]) {
            // Extend the end boundary
            result.back()[1] = max(result.back()[1], intervals[i][1]);
        } else {
            // No overlap â†’ add as new interval
            result.push_back(intervals[i]);
        }
    }
    return result;
}





int main()
{
    // int n ;
    // cin>>n;
    // vector<vector<int>>intervals(n,vector<int>(2));
    // //as n rows and start end 2 coloums
    
    // for(int i = 0 ; i<n ; i++){
    //     cin>>intervals[i][0]>>intervals[i][1];
    // }
    
    // auto merged = mergeInterBrute(intervals);
    // for(auto interval : merged){
    //     cout<<interval[0]<<" "<<interval[1]<<"\n";
    // }
    
    
        int N;
    cin >> N;
    vector<vector<int>> intervals(N, vector<int>(2));
    for (int i = 0; i < N; i++) {
        cin >> intervals[i][0] >> intervals[i][1];
    }

    auto merged = mergeIntervals(intervals);
    for (auto interval : merged) {
        cout << interval[0] << " " << interval[1] << "\n";
    }


    return 0;
}
